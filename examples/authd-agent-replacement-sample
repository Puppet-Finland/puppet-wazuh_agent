<!-- Client Authentication Settings -->
<auth>
  <!-- turn auth daemon on or off -->
  <disabled>no</disabled>
  <port>1515</port>
  <remote_enrollment>yes</remote_enrollment>
  <!-- Toggles the use of the agents source IP address or the use of 'any' -->
  <use_source_ip>no</use_source_ip>
  <!-- Agent replacement options are configured here -->
  <!-- All conditions must be satisfied for replamcement to happend -->
  <force>
    <!-- whether or not to force the insertion of an agent if there is a duplicate name or IP address. -->
    <!-- This will remove the old agent with the same name or IP address. -->
    <enabled>yes</enabled>
    <!-- agent replacement occurs when the key held by the agent is different from the one registered by the manager. -->
    <key_mismatch>yes</key_mismatch>
    <!-- specifies that the replacement will be performed only for agents -->
    <!-- that have been disconnected longer than the value configured in the setting. -->
    <!-- This option should be disabled to replace any agent regardless of its state. -->
    <disconnected_time enabled="no">1s</disconnected_time> -->
    <!-- agent replacement will be performed only when the time passed since the  -->
    <!-- agent registration is greater than the value configured in the setting -->
    <after_registration_time>1m</after_registration_time>
  </force>
  <!-- Toggles the deletion of client keys on or off when agents are removed. -->
  <purge>yes</purge>
  <!-- Toggles shared password authentication on or off .-->
  <use_password>yes</use_password>
  <limit_maxagents>yes</limit_maxagents>
  <ciphers>HIGH:!ADH:!EXP:!MD5:!RC4:!3DES:!CAMELLIA:@STRENGTH</ciphers>
  <ssl_verify_host>no</ssl_verify_host>
  <ssl_manager_cert>/var/ossec/etc/sslmanager.cert</ssl_manager_cert>
  <ssl_manager_key>/var/ossec/etc/sslmanager.key</ssl_manager_key>
  <ssl_auto_negotiate>no</ssl_auto_negotiate>
</auth>
